{
    "name": "yiisoft/yii2-app-advanced",
    "description": "Yii 2 Advanced Project Template",
    "keywords": ["yii2", "framework", "advanced", "project template"],
    "homepage": "http://www.yiiframework.com/",
    "type": "project",
    "license": "BSD-3-Clause",
    "support": {
        "issues": "https://github.com/yiisoft/yii2/issues?state=open",
        "forum": "http://www.yiiframework.com/forum/",
        "wiki": "http://www.yiiframework.com/wiki/",
        "irc": "irc://irc.freenode.net/yii",
        "source": "https://github.com/yiisoft/yii2"
    },
    "minimum-stability": "stable",
    "require": {
        "php": ">=7.2",
        "yiisoft/yii2": ">=2.0.10",
        "yiisoft/yii2-bootstrap": "*",
        "yiisoft/yii2-swiftmailer": "*",
        "yiisoft/yii2-authclient": "*",
        "yiisoft/yii2-jui": "~2.0.0",

        "codemix/yii2-localeurls": "^1.3",
        "himiklab/yii2-recaptcha-widget" : "*",
        "bower-asset/bootbox": "4.3.0",
        "yiisoft/yii2-imagine": "~2.0.0",
        "kartik-v/yii2-widget-fileinput": "@dev",
        "mihaildev/yii2-ckeditor": "*",
        "mihaildev/yii2-elfinder": "*",
        "cebe/yii2-gravatar": "*",
        "kartik-v/yii2-field-range": "*",
        "kartik-v/yii2-helpers": "dev-master",
        "kartik-v/yii2-widget-datetimepicker": "*",
        "kartik-v/yii2-widget-datepicker": "*",
        "kartik-v/yii2-widget-rating": "*",


        "kartik-v/yii2-grid": "@dev",
        "kartik-v/yii2-mpdf": "*",

        "creocoder/yii2-nested-sets": "0.9.*",
        "cakebake/yii2-bootstrap-select": "*",
        "nex/yii2-chosen" : "dev-master",
        "kartik-v/yii2-social": "@dev",

        "nirvana-msu/yii2-infinite-scroll": "1.0.*@dev",
        "codeception/codeception": "*",
        "codeception/specify": "*",
        "codeception/verify": "*",
        "richardfan1126/yii2-js-register":"*",
        "scotthuangzl/yii2-google-chart": "dev-master",
        "matthiasmullie/minify": "^1.3",
        "paypal/rest-api-sdk-php":"*",
        "hail812/yii2-adminlte3": "*"
    },
    "require-dev": {
        "yiisoft/yii2-debug": "*",
        "yiisoft/yii2-gii": "*",
        "yiisoft/yii2-faker": "*",
        "codeception/module-asserts": "^1.3",
        "codeception/module-yii2": "^1.1",
        "codeception/module-filesystem": "^1.0",
        "codeception/module-webdriver": "^1.2",
        "codeception/module-db": "^1.1",
        "codeception/module-rest": "^1.3",
        "codeception/module-phpbrowser": "^1.0"
    },
    "config": {
        "fxp-asset": {
            "installer-paths": {
                "npm-asset-library": "vendor/npm",
                "bower-asset-library": "vendor/bower"
            },
            "repositories": {
                "bower-asset/eve": {
                    "type": "bower-vcs",
                    "url": "https://github.com/adobe-webplatform/eve.git"
                }
            }
        },
        "process-timeout": 1800,
        "discard-changes": true
    },
    "extra": {
    },
    "scripts": {
        "post-install-cmd": "php init --env=Development --overwrite=n"
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://asset-packagist.org"
        }
    ]
}
